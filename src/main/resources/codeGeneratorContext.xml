<?xml version="1.0" encoding="UTF-8"?>
<configContext>

    <jdbc>
        <driver>com.mysql.jdbc.Driver</driver>
        <url>jdbc:mysql://127.0.0.1:3306/test?useUnicode=true&amp;characterEncoding=UTF-8&amp;zeroDateTimeBehavior=convertToNull</url>
        <userName>root</userName>
        <password>root</password>
    </jdbc>


    <path>
        <output>../code</output>
    </path>

    <target>
        <package>com.bj58.supin.das.kefudb</package>
        <dbToCode>
            <entry>
                <key>tl_s_order</key>
                <value>Order</value>
            </entry>
            <entry>
                <key>tl_s_test</key>
                <value>Test</value>
            </entry>
        </dbToCode>
    </target>

    <vm>
        <elements>
            <entry>
                <key>entity.vm</key>
                <value>%s.java</value>
            </entry>
            <entry>
                <key>dao.vm</key>
                <value>%sDao.java</value>
            </entry>
            <entry>
                <key>service.vm</key>
                <value>%sService.java</value>
            </entry>
        </elements>
    </vm>


</configContext>